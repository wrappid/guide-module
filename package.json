{
  "name": "guide-module",
  "version": "0.0.1",
  "description": "This is a template repository for a guide module.",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "code:lint:app": "eslint -c ./.eslint/app/.eslintrc.js --ignore-path .gitignore ./app",
    "code:format:app": "npm run code:lint:app -- --fix ./app",
    "code:lint:service": "eslint -c ./.eslint/service/.eslintrc.js --ignore-path .gitignore ./service",
    "code:format:service": "npm run code:lint:service -- --fix ./service",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git:https://github.com/wrappid/guide-module.git"
  },
  "keywords": [
    "wrappid",
    "module",
    "react",
    "react-native",
    "template"
  ],
  "author": "wrappid",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt-cli": "^17.5.0",
    "@wrappid/core": "latest",
    "@wrappid/styles": "latest",
    "eslint": "^8.40.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "react-redux": "^8.0.5"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "jest": "^27.5.1",
    "service-core": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts}",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}